<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".SplashScreen"
        android:background="@color/backgroundColor"

>

    <ImageView
            android:layout_width="190dp"
            android:layout_height="48dp" app:srcCompat="@drawable/dawn_full"
            android:id="@+id/imageView2"
            app:layout_constraintTop_toTopOf="parent" android:layout_marginBottom="8dp"
            app:layout_constraintBottom_toBottomOf="parent" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginLeft="8dp" android:layout_marginStart="8dp" app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginEnd="8dp" android:layout_marginRight="8dp" app:layout_constraintVertical_bias="0.338"
            app:layout_constraintHorizontal_bias="0.497" android:layout_marginTop="8dp"/>
    <ImageView
            android:layout_width="261dp"
            android:layout_height="51dp" app:srcCompat="@drawable/white_line"
            android:id="@+id/imageView9"
            app:layout_constraintStart_toStartOf="parent" android:layout_marginLeft="8dp"
            android:layout_marginStart="8dp" app:layout_constraintEnd_toEndOf="parent" android:layout_marginEnd="8dp"
            android:layout_marginRight="8dp" android:layout_marginTop="272dp"
            app:layout_constraintTop_toTopOf="parent"/>
    <TextView
            android:layout_width="338dp"
            android:layout_height="20dp"
            android:id="@+id/textView" app:layout_constraintStart_toStartOf="parent" android:layout_marginLeft="8dp"
            android:layout_marginStart="8dp" app:layout_constraintEnd_toEndOf="parent" android:layout_marginEnd="8dp"
            android:layout_marginRight="8dp" app:layout_constraintTop_toTopOf="parent" android:layout_marginTop="440dp"
            app:layout_constraintHorizontal_bias="0.491" android:text="@string/splash_screen_tagline_1"
            android:textColor="@color/textColor" android:gravity="center" android:textSize="16sp"/>
    <TextView
            android:text="@string/splash_screen_tagline_2"
            android:layout_width="361dp"
            android:layout_height="20dp"
            android:id="@+id/textView2" app:layout_constraintStart_toStartOf="parent" android:layout_marginLeft="8dp"
            android:layout_marginStart="8dp" app:layout_constraintEnd_toEndOf="parent" android:layout_marginEnd="8dp"
            android:layout_marginRight="8dp"
            app:layout_constraintTop_toBottomOf="@+id/textView" android:textColor="@color/textColor"
            android:gravity="center" android:textSize="16sp"/>
</androidx.constraintlayout.widget.ConstraintLayout>